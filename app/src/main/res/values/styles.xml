<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="MyCustomTheme" parent="style/AppTheme">
        <item name="android:actionOverflowButtonStyle">@style/MyCustomTheme.OverFlow</item>
    </style>

    <style name="MyCustomTheme.OverFlow" parent="@android:style/Widget.ActionButton.Overflow">
        <item name="android:src">@drawable/ic_action_name</item>
    </style>

</resources>
